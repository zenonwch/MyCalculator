<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<TableView prefHeight="400.0" prefWidth="250.0"
           xmlns="http://javafx.com/javafx/8.0.121"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.vaz.projects.calculator.controller.Shortcuts"
           fx:id="tableView">
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
    <columns>
        <TableColumn prefWidth="${tableView.width * 0.25}" minWidth="60.0" text="Key" fx:id="keys"/>
        <TableColumn prefWidth="${tableView.width * 0.74}" minWidth="160.0" text="Action" fx:id="actions"/>
    </columns>
</TableView>
